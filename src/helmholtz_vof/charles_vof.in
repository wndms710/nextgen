VERBOSE

RESTART ../stitch/restart.mles
DEFINE N 8
#SIMPLE_RESTART CART 0 $(N) 0 1 0 1 $(N) 1 1
#SIMPLE_RESTART CART 0 1 0 1 0 1 $(N) $(N) $(N)
