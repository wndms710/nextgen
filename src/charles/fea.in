#RESTART unit_cube.tet_bin #T.cht
#METRIC_TO_ENGLISH
SINGLE_TET

#WRITE_INITIAL_MESH

#PIN FACE X0 true false false
#PIN FACE Y0 false true false
#PIN FACE Z0 false false true
PIN POINT 0 0 0 true true true
PIN POINT 0 1 0 true false true 
PIN POINT 0 0 1 true false false
#PIN DIR Y
#PIN DIR Z

T0 0 # reference temperature

MAXITER 1000
VERBOSE true
ZERO 1.0E-6

CHT.E CONSTANT 1
CHT.NU CONSTANT 0
CHT.ALPHA CONSTANT 1
#CHT.0.E CONSTANT 1
#CHT.0.NU CONSTANT 0
#CHT.0.ALPHA CONSTANT 1
#CHT.1.E CONSTANT 1
#CHT.1.NU CONSTANT 0
#CHT.1.ALPHA CONSTANT 0.5

T_PROFILE LINEAR
#SPLIT_ZONE 
